{% extends 'base.html.twig' %}

{% block body %}

    {% include ('Shared/loginmenu.html.twig') %}

    <p>{{ app_group.name }}</p>

        <ul>
            {% for item in app_group.appUsers %}
                <li><a href="{{ path('user_show', {'slug': item.id}) }}"> {{ item.username }}</a> </li>
            {% endfor %}
        </ul>

{% endblock %}