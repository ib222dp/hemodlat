{% extends 'base.html.twig' %}

{% block body %}

    {% include ('Shared/loginmenu.html.twig') %}

    <p>{{ location.name }}</p>
    <ul>
        {% for item in location.appUsers %}
            <li><a href="{{ path('user_show', {'slug': item.id}) }}"> {{ item.username }} </a></li>
        {% endfor %}
    </ul>

{% endblock %}