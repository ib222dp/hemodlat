{% extends 'base.html.twig' %}

{% block body %}
    <ul id="locations">
        {% for item in locations %}
            <li><a href="{{ path('location_show', {'slug': item.id}) }}">{{ item.name }} </a></li>
        {% endfor %}
    </ul>
    <a href="{{ path('logout') }}">Logga ut</a>
{% endblock %}